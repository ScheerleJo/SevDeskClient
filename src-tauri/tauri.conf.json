{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "config.json"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "SevDeskExtension-Client",
  "mainBinaryName": "SevDeskExtension-Client",
  "version": "1.1.1",
  "identifier": "com.tauri.sevDeskExtension.Client",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "SevDeskExtension-Client",
        "width": 1400,
        "height": 800,
        "decorations": true,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}